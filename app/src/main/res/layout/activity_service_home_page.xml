<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.dev.r19.localservicefinder.ServiceHomePage"
    android:background="@drawable/background1">


    <TextView
        android:id="@+id/textView11"
        android:layout_width="260dp"
        android:layout_height="60dp"
        android:text="Welcome To Your Home."
        android:textColor="#ffffff"
        android:textSize="22dp"
        android:textStyle="bold"
        android:layout_centerVertical="true"
        android:layout_centerHorizontal="true" />

    <Button
        android:id="@+id/ServiceSignOut"
        android:layout_width="160dp"
        android:layout_height="45dp"
        android:layout_marginLeft="50dp"
        android:background="@drawable/btn_back6"
        android:layout_marginTop="33dp"
        android:text="Log Out"
        android:textStyle="bold"
        android:textColor="#ffffff"
        android:layout_marginStart="21dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true" />

    <Button
        android:id="@+id/ToAddProfile"
        android:layout_width="160dp"
        android:layout_height="45dp"
        android:textColor="#ffffff"
        android:layout_alignParentEnd="true"
        android:background="@drawable/btn_back6"
        android:layout_alignTop="@+id/ServiceSignOut"
        android:layout_marginEnd="30dp"
        android:text="Edit Your Profile" />
</RelativeLayout>
