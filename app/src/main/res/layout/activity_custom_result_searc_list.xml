<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.dev.r19.localservicefinder.CustomResultSearcList">

    <LinearLayout
        android:layout_width="390dp"
        android:layout_height="match_parent"
        android:layout_marginStart="32dp"
        android:orientation="vertical"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true">
        <TextView
            android:id="@+id/textView49"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="32dp"
            android:layout_marginTop="38dp"
            android:text="List of available Service Proviedr as per your criteria "
            android:layout_alignParentTop="true"
            android:layout_alignParentStart="true" />

        <ListView
            android:id="@+id/searclist"
            android:layout_width="340dp"
            android:layout_height="match_parent"
            android:layout_alignStart="@+id/textView49"
            android:layout_below="@+id/textView49"
            android:layout_marginTop="81dp" />

    </LinearLayout>
</RelativeLayout>
