<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dev.r19.localservicefinder">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ClientCredentials"></activity>
        <activity android:name=".ClientLogin"></activity>
        <activity android:name=".ClientRegister"></activity>
        <activity android:name=".Home"></activity>
        <activity android:name=".ServiceCredentials"></activity>
        <activity android:name=".ServiceLogin"></activity>
        <activity android:name=".ServiceRegister"></activity>
    </application>

</manifest>